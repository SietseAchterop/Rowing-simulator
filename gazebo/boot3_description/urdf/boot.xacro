<?xml version="1.0" ?>

<robot name="myboat"  xmlns:xacro="http://www.ros.org/wiki/xacro">

  <!-- Set some joints to fixed when recreating the kinematic loops again -->
  <xacro:arg name="reconnectloops" default="false" />
  <xacro:if value="$(arg reconnectloops)">
    <xacro:property name="sliding_type" value="fixed" />
    <xacro:property name="hip2_type" value="fixed" />
    <xacro:property name="starboard_rowlock_type" value="fixed" />
    <xacro:property name="port_rowlock_type" value="fixed" />
    <xacro:property name="starboard_oar_type" value="fixed" />
    <xacro:property name="port_oar_type" value="fixed" />
  </xacro:if>
  <xacro:unless value="$(arg reconnectloops)">
    <xacro:property name="sliding_type" value="prismatic" />
    <xacro:property name="hip2_type" value="revolute" />
    <xacro:property name="starboard_rowlock_type" value="revolute" />
    <xacro:property name="port_rowlock_type" value="revolute" />
    <xacro:property name="starboard_oar_type" value="revolute" />
    <xacro:property name="port_oar_type" value="revolute" />
  </xacro:unless>

  <!-- Import all Gazebo-customization elements, including Gazebo colors -->
  <xacro:include filename="$(find boot3_description)/urdf/boot.gazebo" />
  <!-- Import Rviz colors -->
  <xacro:include filename="$(find boot3_description)/urdf/materials.xacro" />

  <!-- parameters that can be configured -->
  <xacro:include filename="$(find boot3_description)/config/param.xacro" />

  <joint name="prism" type="prismatic">
    <parent link="world"/>
    <child link="l1"/>
    <origin xyz="0 0 0.25" rpy="0 0 0"/>
    <dynamics damping="0.005" />
    <axis xyz="1 0 0"/>
    <limit effort="101.0" lower="0" upper="1000" velocity="0.5"/>
  </joint>

  <joint name="rev1" type="revolute">
    <parent link="l1"/>
    <child link="l2"/>
    <origin xyz="0.0 0 -0.2" rpy="0 ${pi/2} 0"/>
    <dynamics damping="0.005" />
    <axis xyz="0 1 0"/>
    <limit effort="101.0" lower="-1" upper="1" velocity="1.0"/>
  </joint>

  <joint name="rev2" type="revolute">
    <parent link="l2"/>
    <child link="skiff"/>
    <origin xyz="0.0 0.0 0.4" rpy="${pi} ${pi/2} 0"/>
    <dynamics damping="0.005" />
    <axis xyz="0 1 0"/>
    <limit effort="101.0" lower="-1" upper="1" velocity="1.0"/>
  </joint>

  <joint name="starboard_rowlock" type="${starboard_rowlock_type}">
    <parent link="skiff"/>
    <child link="starboard_rowlock_l"/>
    <origin rpy="0.0  0.0 ${starboard_rowlock}" xyz="-3 ${span/2} 0.27"/>
    <dynamics damping="0.005" />
    <axis xyz="0 0 1"/>
    <limit effort="101.0" lower="-0.4" upper="1.4" velocity="1.0"/>
  </joint>

  <joint name="port_rowlock" type="${port_rowlock_type}">
    <parent link="skiff"/>
    <child link="port_rowlock_l"/>
    <origin rpy="0.0  0.0 ${port_rowlock}" xyz="-3 -${span/2} 0.25"/>
    <dynamics damping="0.005" />
    <axis xyz="0 0 1"/>
    <limit effort="101.0" lower="-1.4" upper="1.4" velocity="1.0"/>
  </joint>

  <joint name="starboard_oar" type="${starboard_oar_type}">
    <parent link="starboard_rowlock_l"/>
    <child link="starboard_oar_l"/>
    <origin rpy="0.0  ${pi/2+starboard_oar} -${pi/2}" xyz="0.0 0.0   0.02"/>
    <dynamics damping="0.005" />
    <axis xyz="0 1 0"/>
    <limit effort="101.0" lower="-0.4" upper="0.1" velocity="1.0"/>
  </joint>

  <joint name="port_oar" type="${port_oar_type}">
    <parent link="port_rowlock_l"/>
    <child link="port_oar_l"/>
    <origin rpy="0.0  ${pi/2+port_oar} -${pi/2}" xyz="0.0 0.0   0.02"/>
    <dynamics damping="0.005" />
    <axis xyz="0 1 0"/>
    <limit effort="101.0" lower="-0.1" upper="0.4" velocity="1.0"/>
  </joint>

  <joint name="starboard_blade" type="revolute">
    <parent link="starboard_oar_l"/>
    <child link="starboard_blade_l"/>
    <origin rpy="0 0  0.0" xyz="0.0 0.0   -${outer}"/>
    <dynamics damping="0.005" />
    <axis xyz="0 1 0"/>
    <limit effort="101.0" lower="-0.4" upper="0.4" velocity="1.0"/>
  </joint>

  <joint name="port_blade" type="revolute">
    <parent link="port_oar_l"/>
    <child link="port_blade_l"/>
    <origin rpy="0.0  0 0.0" xyz="0.0 0.0   ${outer}"/>
    <dynamics damping="0.005" />
    <axis xyz="0 1 0"/>
    <limit effort="101.0" lower="-0.4" upper="0.4" velocity="1.0"/>
  </joint>

  <!-- Rower joints -->

  <joint name="sk_fp" type="fixed">
    <parent link="skiff"/>
    <child link="footplate"/>
    <origin rpy="0.0  0 0.0" xyz="-2.5 0.0  0.1"/>
    <dynamics damping="0.005" />
    <limit effort="101.0" lower="-0.4" upper="0.4" velocity="1.0"/>
  </joint>

  <joint name="sliding" type="${sliding_type}">  <!-- prismatic -->
    <parent link="skiff"/>
    <child link="seat"/>
    <origin rpy="0 0 0" xyz="-3.49514 0.0 0.1"/>
    <dynamics damping="0.005" />
    <axis xyz="1 0 0"/>
    <limit effort="101.0" lower="-0.1" upper="0.8" velocity="0.5"/>
  </joint>

  <joint name="fp_ll" type="revolute">
    <parent link="footplate"/>
    <child link="lowerleg"/>
    <origin rpy="0.0  0.1001675 0.0" xyz="0.0 0.0   0.0"/>
    <dynamics damping="0.005" />
    <axis xyz="0 1 0"/>
    <limit effort="101.0" lower="0.0" upper="${pi/2}" velocity="1.0"/>
  </joint>

  <joint name="knee" type="revolute">
    <parent link="lowerleg"/>
    <child link="upperleg_1"/>
    <origin rpy="0.0  0 0.0" xyz="-0.5 0.0   0.0"/>
    <dynamics damping="0.005" />
    <axis xyz="0 1 0"/>
    <limit effort="1000.0" lower="-3.1" upper="0" velocity="1.0"/>
  </joint>

  <joint name="hip1" type="revolute">
    <parent link="seat"/>
    <child link="upperleg_2"/>
    <origin rpy="0  0.1001675 0.0" xyz="0.0 0.0   0.1"/>
    <dynamics damping="0.005" />
    <axis xyz="0 1 0"/>
    <limit effort="101.0" lower="-1.6" upper="0" velocity="1.0"/>
  </joint>

  <joint name="hip2" type="${hip2_type}"> 
    <parent link="seat"/>
    <child link="back_low"/>
    <origin rpy="0  -${pi/2+hip2} 0.0" xyz="0.0 0.0   0.1"/>
    <dynamics damping="0.005" />
    <axis xyz="0 1 0"/>
    <limit effort="1000.0" lower="-1.0" upper="1.0" velocity="1.0"/>
  </joint>

  <joint name="back_lh" type="revolute">  
    <parent link="back_low"/>
    <child link="back_high"/>
    <origin rpy="0  ${back_lh} 0.0" xyz="0.25 0.0   0.0"/>
    <dynamics damping="0.5" />
    <axis xyz="0 1 0"/>
    <limit effort="101.0" lower="0.0" upper="0.5" velocity="1.0"/>
  </joint>

  <joint name="shoulder" type="fixed">
    <parent link="back_high"/>
    <child link="shoulder_link"/>
    <origin rpy="0  0.0 0.0" xyz="0.3 0.0   0.0"/>
    <dynamics damping="0.5" />
    <limit effort="101.0" lower="0.0" upper="0.5" velocity="1.0"/>
  </joint>

  <joint name="head_joint" type="fixed">
    <parent link="shoulder_link"/>
    <child link="head"/>
    <origin rpy="0  0.0 0.0" xyz="0.2 0.0   0.0"/>
  </joint>

  <!-- Left branch from starboard_oar to shoulder_left -->
  
  <!-- 3D handle joint left -->
  <joint name="handle_left_j1" type="revolute">
    <parent link="starboard_oar_l"/>
    <child link="handle_left_l1"/>
    <origin rpy="${handle_left_j1} 0 ${pi/2}" xyz="0 0.0 ${inner}"/>
    <dynamics damping="0.005" />
    <axis xyz="1 0 0"/>
    <limit effort="101.0" lower="-1" upper="1" velocity="1.0"/>
  </joint>
  <link name="handle_left_l1">
    <inertial>
      <mass value="0.01"/>
      <origin rpy="0 0 0" xyz="0 0.0 0.0"/>
      <inertia ixx="0.000000167" ixy="0" ixz="0" iyy="0.000000167" iyz="0" izz="0.000000167"/> 
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0.0 0.0 0.0"/>
      <geometry>
        <box size="0.01 0.01 0.01"/>
      </geometry>
    </visual>
  </link>
  <joint name="handle_left_j2" type="revolute">
    <parent link="handle_left_l1"/>
    <child link="handle_left_l2"/>
    <origin rpy="0 0 ${handle_left_j2}" xyz="0 0.0 0.0"/>
    <dynamics damping="0.005" />
    <axis xyz="0 0 1"/>
    <limit effort="101.0" lower="-1" upper="1" velocity="1.0"/>
  </joint>
  <link name="handle_left_l2">
    <inertial>
      <mass value="0.01"/>
      <origin rpy="0 0 0" xyz="0 0.0 0.0"/>
      <inertia ixx="0.000000167" ixy="0" ixz="0" iyy="0.000000167" iyz="0" izz="0.000000167"/> 
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0.0 0.0 0.0"/>
      <geometry>
        <box size="0.01 0.01 0.01"/>
      </geometry>
    </visual>
  </link>
  <joint name="handle_left_j3" type="revolute">
    <parent link="handle_left_l2"/>
    <child link="arm_l_lower"/>
    <origin rpy="0 ${handle_left_j3} 0" xyz="0 0.0 0.0"/>
    <dynamics damping="0.005" />
    <axis xyz="0 1 0"/>
    <limit effort="101.0" lower="-1" upper="1" velocity="1.0"/>
  </joint>

  <!-- 3D joint left elbow -->
  <joint name="elbow_left" type="revolute">
    <parent link="arm_l_lower"/>
    <child link="left_elbow_psi_link"/>
    <origin rpy="0 0 ${elbow_left}" xyz="-0.4 0.0 0.0"/>
    <dynamics damping="0.5" />
    <axis xyz="0 0 1"/>
    <limit effort="101.0" lower="-1.0" upper="2.0" velocity="1.0"/>
  </joint>
  <link name="left_elbow_psi_link">
    <inertial>
      <mass value="0.01"/>
      <origin rpy="0 0 0" xyz="0 0.0 0.0"/>
      <inertia ixx="0.000000167" ixy="0" ixz="0" iyy="0.000000167" iyz="0" izz="0.000000167"/> 
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0.0 0.0 0.0"/>
      <geometry>
        <box size="0.01 0.01 0.01"/>
      </geometry>
    </visual>
  </link>
  <joint name="elbow_theta_l" type="revolute" >
    <parent link="left_elbow_psi_link"/>
    <child link="left_elbow_theta_link"/>
    <origin rpy="0.0 ${elbow_theta_l}  0.0" xyz="-0.0 0.0  0.0"/>
    <axis xyz="0 1 0" />
    <dynamics damping="0.5" />
    <limit effort="101.0" lower="-0.7" upper="2.0" velocity="1.0"/>
  </joint>
  <link name="left_elbow_theta_link">
    <inertial>
      <mass value="0.01"/>
      <origin rpy="0 0 0" xyz="0 0.0 0.0"/>
      <inertia ixx="0.000000167" ixy="0" ixz="0" iyy="0.000000167" iyz="0" izz="0.000000167"/> 
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0.0 0.0 0.0"/>
      <geometry>
        <box size="0.01 0.01 0.01"/>
      </geometry>
    </visual>
  </link>
  <joint name="elbow_xx_l" type="revolute" >
    <parent link="left_elbow_theta_link"/>
    <child link="arm_l_upper"/>
    <origin rpy="${elbow_xx_l} 0.0  0.0" xyz="-0.0 0.0  0.0"/>
    <axis xyz="1 0 0" />
    <dynamics damping="0.5" />
    <limit effort="101.0" lower="-0.7" upper="2.0" velocity="1.0"/>
  </joint>

  <!-- 3D joint left shoulder -->
  <joint name="shoulder_theta_l" type="revolute" >
    <parent link="arm_l_upper"/>
    <child link="left_shoulder_psi_link"/>
    <origin rpy="0 ${shoulder_theta_l} 0" xyz="-0.35 0.0 0.0"/>
    <axis xyz="0 1 0" />
    <dynamics damping="0.005" />
    <limit effort="101.0" lower="-1.0" upper="1.0" velocity="1.0"/>
  </joint>
  <link name="left_shoulder_psi_link">
    <inertial>
      <mass value="0.01"/>
      <origin rpy="0 0 0" xyz="0 0.0 0.0"/>
      <inertia ixx="0.000000167" ixy="0" ixz="0" iyy="0.000000167" iyz="0" izz="0.000000167"/> 
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0.0 0.0 0.0"/>
      <geometry>
        <box size="0.01 0.01 0.01"/>
      </geometry>
    </visual>
  </link>
  <joint name="shoulder_psi_l" type="revolute" >
    <parent link="left_shoulder_psi_link" />
    <child link="left_shoulder_xx_link" />
    <origin rpy="${shoulder_psi_l} 0.0 0.0" xyz="0.0 0.0   0.0"/>
    <axis xyz="1 0 0" />
    <dynamics damping="0.005" />
    <limit effort="101.0" lower="-1.0" upper="1.0" velocity="1.0"/>
  </joint>
  <link name="left_shoulder_xx_link">
    <inertial>
      <mass value="0.01"/>
      <origin rpy="0 0 0" xyz="0 0.0 0.0"/>
      <inertia ixx="0.000000167" ixy="0" ixz="0" iyy="0.000000167" iyz="0" izz="0.000000167"/> 
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0.0 0.0 0.0"/>
      <geometry>
        <box size="0.01 0.01 0.01"/>
      </geometry>
    </visual>
  </link>
  <joint name="shoulder_l" type="revolute" >
    <parent link="left_shoulder_xx_link" />
    <child link="shoulder_left" />
    <origin rpy="0.0 0.0 ${shoulder_l}" xyz="0.0 0.0   0.0"/>
    <axis xyz="0 0 1" />
    <dynamics damping="0.005" />
    <limit effort="101.0" lower="-1.0" upper="1.0" velocity="1.0"/>
  </joint>
  <joint name="Ldummy_2" type="fixed">
    <parent link="shoulder_left"/>
    <child link="Ldummy_link_2"/>
    <origin rpy="0 0 ${pi} " xyz="0.0 0.1 0.0"/>
    <dynamics damping="0.005" />
    <limit effort="101.0" lower="-1.0" upper="1.0" velocity="1.0"/>
  </joint>
  <link name="Ldummy_link_2">
    <inertial>
      <mass value="0.01"/>
      <origin rpy="0 0 0" xyz="0 0.0 0.0"/>
      <inertia ixx="0.000000167" ixy="0" ixz="0" iyy="0.000000167" iyz="0" izz="0.000000167"/> 
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0.0 0.0 0.0"/>
      <geometry>
        <box size="0.01 0.01 0.01"/>
      </geometry>
    </visual>
  </link>
  
  <!-- in gazebo here the joint that closes the left loop -->

  <joint name="Ldummy_1" type="fixed">
    <parent link="shoulder_link"/>
    <child link="Ldummy_link_1"/>
    <origin rpy="0 0 0.0" xyz="0.0 0.1 0.0"/>
    <dynamics damping="0.005" />
    <limit effort="101.0" lower="-1.0" upper="1.0" velocity="1.0"/>
  </joint>
  <link name="Ldummy_link_1">
    <inertial>
      <mass value="0.01"/>
      <origin rpy="0 0 0" xyz="0 0.0 0.0"/>
      <inertia ixx="0.000000167" ixy="0" ixz="0" iyy="0.000000167" iyz="0" izz="0.000000167"/> 
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0.0 0.0 0.0"/>
      <geometry>
        <box size="0.01 0.01 0.01"/>
      </geometry>
    </visual>
  </link>


  <!-- Right branch from port_oar to shoulder_right -->

  <!-- 3D handle joint right-->
  <joint name="handle_right_j1" type="revolute">
    <parent link="port_oar_l"/>
    <child link="handle_right_l1"/>
    <origin rpy="${-1*handle_right_j1} 0 ${pi/2}" xyz="0 0.0 -${inner}"/>
    <dynamics damping="0.005" />
    <axis xyz="-1 0 0"/>
    <limit effort="101.0" lower="-1" upper="1" velocity="1.0"/>
  </joint>
  <link name="handle_right_l1">
    <inertial>
      <mass value="0.01"/>
      <origin rpy="0 0 0" xyz="0 0.0 0.0"/>
      <inertia ixx="0.000000167" ixy="0" ixz="0" iyy="0.000000167" iyz="0" izz="0.000000167"/> 
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0.0 0.0 0.0"/>
      <geometry>
        <box size="0.01 0.01 0.01"/>
      </geometry>
    </visual>
  </link>
  <joint name="handle_right_j2" type="revolute">
    <parent link="handle_right_l1"/>
    <child link="handle_right_l2"/>
    <origin rpy="0 0 ${handle_right_j2}" xyz="0 0.0 0.0"/>
    <dynamics damping="0.005" />
    <axis xyz="0 0 1"/>
    <limit effort="101.0" lower="-1" upper="1" velocity="1.0"/>
  </joint>
  <link name="handle_right_l2">
    <inertial>
      <mass value="0.01"/>
      <origin rpy="0 0 0" xyz="0 0.0 0.0"/>
      <inertia ixx="0.000000167" ixy="0" ixz="0" iyy="0.000000167" iyz="0" izz="0.000000167"/> 
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0.0 0.0 0.0"/>
      <geometry>
        <box size="0.01 0.01 0.01"/>
      </geometry>
    </visual>
  </link>
  <joint name="handle_right_j3" type="revolute">
    <parent link="handle_right_l2"/>
    <child link="arm_r_lower"/>
    <origin rpy="0 ${-1*handle_right_j3} 0" xyz="0 0.0 0.0"/>
    <dynamics damping="0.005" />
    <axis xyz="0 -1 0"/>
    <limit effort="101.0" lower="-1" upper="1" velocity="1.0"/>
  </joint>


  <!-- 3D joint right elbow -->
  <joint name="elbow_right" type="revolute">
    <parent link="arm_r_lower"/>
    <child link="right_elbow_psi_link"/>
    <origin rpy="0 0 ${elbow_right}" xyz="-0.4 0.0 0.0"/>
    <dynamics damping="0.5" />
    <axis xyz="0 0 1"/>
    <limit effort="101.0" lower="-1.0" upper="1.0" velocity="1.0"/>
  </joint>
  <link name="right_elbow_psi_link">
    <inertial>
      <mass value="0.01"/>
      <origin rpy="0 0 0" xyz="0 0.0 0.0"/>
      <inertia ixx="0.000000167" ixy="0" ixz="0" iyy="0.000000167" iyz="0" izz="0.000000167"/> 
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0.0 0.0 0.0"/>
      <geometry>
        <box size="0.01 0.01 0.01"/>
      </geometry>
    </visual>
  </link>
  <joint name="elbow_theta_r" type="revolute" >
    <parent link="right_elbow_psi_link"/>
    <child link="right_elbow_theta_link"/>
    <origin rpy="0.0 ${elbow_theta_r}  0.0" xyz="0.0 0.0  0.0"/>
    <axis xyz="0 1 0" />
    <dynamics damping="0.5" />
    <limit effort="101.0" lower="-2.0" upper="1.0" velocity="1.0"/>
  </joint>
  <link name="right_elbow_theta_link">
    <inertial>
      <mass value="0.01"/>
      <origin rpy="0 0 0" xyz="0 0.0 0.0"/>
      <inertia ixx="0.000000167" ixy="0" ixz="0" iyy="0.000000167" iyz="0" izz="0.000000167"/> 
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0.0 0.0 0.0"/>
      <geometry>
        <box size="0.01 0.01 0.01"/>
      </geometry>
    </visual>
  </link>
  <joint name="elbow_xx_r" type="revolute" >
    <parent link="right_elbow_theta_link"/>
    <child link="arm_r_upper"/>
    <origin rpy="${-1*elbow_xx_r} 0.0  0.0" xyz="0.0 0.0  0.0"/>
    <axis xyz="-1 0 0" />
    <dynamics damping="0.5" />
    <limit effort="101.0" lower="-1.0" upper="2.0" velocity="1.0"/>
  </joint>


  <!-- 3D joint right shoulder -->
  <joint name="shoulder_theta_r" type="revolute" >
    <parent link="arm_r_upper"/>
    <child link="right_shoulder_psi_link"/>
    <origin rpy="${pi} ${-1*shoulder_theta_r} 0" xyz="0.35 0.0 0.0"/>
    <axis xyz="0 -1 0" />
    <dynamics damping="0.5" />
    <limit effort="101.0" lower="-1.0" upper="1.0" velocity="1.0"/>
  </joint>
  <link name="right_shoulder_psi_link">
    <inertial>
      <mass value="0.01"/>
      <origin rpy="0 0 0" xyz="0 0.0 0.0"/>
      <inertia ixx="0.000000167" ixy="0" ixz="0" iyy="0.000000167" iyz="0" izz="0.000000167"/> 
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0.0 0.0 0.0"/>
      <geometry>
        <box size="0.01 0.01 0.01"/>
      </geometry>
    </visual>
  </link>
  <joint name="shoulder_psi_r" type="revolute" >
    <parent link="right_shoulder_psi_link" />
    <child link="right_shoulder_xx_link" />
    <origin rpy="${shoulder_psi_r} 0.0 0.0" xyz="0.0 0.0   0.0"/>
    <axis xyz="1 0 0" />
    <dynamics damping="0.005" />
    <limit effort="101.0" lower="-1.0" upper="1.0" velocity="1.0"/>
  </joint>
  <link name="right_shoulder_xx_link">
    <inertial>
      <mass value="0.01"/>
      <origin rpy="0 0 0" xyz="0 0.0 0.0"/>
      <inertia ixx="0.000000167" ixy="0" ixz="0" iyy="0.000000167" iyz="0" izz="0.000000167"/> 
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0.0 0.0 0.0"/>
      <geometry>
        <box size="0.01 0.01 0.01"/>
      </geometry>
    </visual>
  </link>
  <joint name="shoulder_r" type="revolute" >
